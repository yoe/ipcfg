#!/bin/sh

# Remove ipcfg init.d script, in favour of the netbase one

if [ "$1" = install ] || [ "$1" = upgrade ]
then
	if [ -e "/etc/init.d/ipcfg" ]
	then
		if [ "`md5sum \"/etc/init.d/ipcfg\" | sed -e \"s/ .*//\"`" = \
		"`dpkg-query -W -f='${Conffiles}' ipcfg | sed -n -e \"\\\\' /etc/init.d/ipcfg's/.* //p\"`" ]
		then
			rm -f /etc/init.d/ipcfg
		fi
	fi
fi

